{
  "name": "countdown3d",
  "version": "1.0.6",
  "description": "A 3D mechanical-style countdown timer built with React and CSS cubes",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:types": "tsc --project tsconfig.build.json",
